n {\n        addPost: (newPostText) => {\n            dispatch(addPostAC(newPostText))\n        }\n    }\n}\n\nconst MyPostsContainer = connect(mapStateToProps, mapDispatchToProps) (MyPosts)\n\nexport default MyPostsContainer\n\n","/home/chynara/Desktop/projects/react-project/myreact.js/react-project/src/components/Dialogs/Message/Message.jsx",[],"/home/chynara/Desktop/projects/react-project/myreact.js/react-project/src/components/Dialogs/DialogsItem/DialogsItem.jsx",[],"/home/chynara/Desktop/projects/react-project/myreact.js/react-project/src/components/Profile/ProfileInfo/ProfileStatus.jsx",[],"/home/chynara/Desktop/projects/react-project/myreact.js/react-project/src/components/Profile/MyPosts/MyPosts.jsx",["171"],"import React from 'react'\nimport s from './MyPosts.module.css'\nimport Post from \"./Post/Post\";\nimport {Field, reduxForm} from \"redux-form\";\nimport {maxLengthCreator, required} from \"../../../utils/validators/validators\";\nimport {Textarea} from \"../../common/FormsControl/FormsControls\";\n\nconst maxLength10 = maxLengthCreator(10)\n\nconst AddNewPostForm = (props) => {\n    return (\n        <form onSubmit={props.handleSubmit}>\n            <div>\n                <Field name={'newPostText'}\n                       component={Textarea}\n                       type={'textarea'}\n                       validate={[required, maxLength10]}\n                       placeholder={'type you message'}\n                />\n            </div>\n            <div>\n                <button>Add Post</button>\n            </div>\n        </form>\n    )\n}\n\nconst AddNewPostFormRedux = reduxForm({form: 'MyPostForm'})(AddNewPostForm)\n\nconst MyPosts = React.memo(props => {\n    let postsElements =\n        props.posts.map(p => <Post message={p.message} likesCount={p.likesCount} key={p.id}/>)\n\n    let newPostElement = React.createRef()\n\n    let onAddPost = (values) => {\n        props.addPost(values.newPostText);\n    }\n\n    return (\n        <div className